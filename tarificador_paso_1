// Insert this script in "Comenzar presupuesto" button
<script type="text/javascript">
window.addEventListener('submit', function (event) { // Listen for form submit event. In this case 'submit' by the correct event (if not sure if is 'click')
    // Trigger postMessage
    parent.postMessage(
    {
        'eventName': 'tarificador_steps',
        'step': 'Before you begin',
    }, 
    "https://adecco.com") // replace with Top Domain
}, false);
</script>
